{
  "name": "zadanie_14_5",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "globstar jshint js/*.js",
    "init-project": "npm init && npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*.css",
    "build:sass": "node-sass --output-style compact -o css sass/style.scss",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange sass/*.scss -- npm-run-all sassprefixer:*",
    "sassprefixer:sass": "node-sass --output-style expanded --source-map true -o css sass/style.scss",
    "sassprefixer:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:browsersync": "browser-sync start --server --files 'css/*.css' '*.html' '*.js' 'components/*.js' --browser \"C:/Program Files/Firefox Developer Edition/firefox.exe\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.7.0",
    "install": "^0.10.4",
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.23.5",
    "globstar": "^1.0.0",
    "jshint": "^2.9.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.8.1",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
